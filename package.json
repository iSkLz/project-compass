  "bugs": {
    "url": "https://github.com/iSkLz/project-compass/issues"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.3",
    "@types/materialize-css": "^1.0.8",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "electron": "^11.2.3",
    "grunt": "^1.3.0",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-uglify": "^5.0.0",
    "grunt-webpack": "^4.0.2",
    "sass-loader": "^11.0.1",
    "grunt-contrib-htmlmin": "^3.1.0",
    "grunt-contrib-sass": "^2.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",

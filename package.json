{
  "name": "celestial-compass-dev",
  "version": "0.0.1",
  "description": "Celestial Compass's development package",
  "main": "main.js",
  "scripts": {
    "ts": "npx tsc -p tsconfig-main.json",
    "tsx": "npx tsc -p tsconfig-web.json",
    "tsall": "npm run ts && npm run tsx",

    "tsw": "npx tsc -p tsconfig-main.json -w",
    "tsxw": "npx tsc -p tsconfig-web.json -w",

    "files": "npx grunt",

    "launch": "npx electron .",
    "build": "npm run ts && npm run tsx && npm run files",
    "run": "npm run build && npm run launch",
    "runfiles": "npm run files && npm run launch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iSkLz/project-compass.git"
  },

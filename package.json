    "jquery": "^3.5.1",
    "materialize-css": "^1.0.0-rc.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "sass": "^1.27.0",
    "semver": "^7.3.4",
    "serialize-error": "^7.0.1",
    "ws": "^7.4.3"
  },
  "homepage": "https://github.com/iSkLz/project-compass#readme"
